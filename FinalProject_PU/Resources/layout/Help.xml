<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bglogin">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tev1"
        android:text="Feedback"
        android:textColor="#fff"
        android:textSize="25sp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"/>
    
   
    <ImageView
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:id="@+id/create_issue4_btnnext"
        android:src="@drawable/iconwhitecomplete"
        android:layout_marginTop="20dp"
        android:layout_alignParentEnd="true"
        />
     <ImageView
        android:src="@drawable/backgo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"   
        android:layout_marginTop="20dp"
        
        android:id="@+id/imgbackgo"
       
         />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text1"
        android:text="Enter Your Feedback"
        android:textColor="#fff"
        android:textSize="25sp"
        android:layout_below="@id/tev1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"/>

    <EditText
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text1"
        android:textColor="#fff"
        android:layout_marginTop="30dp"
        android:maxLines="3"
        android:id="@+id/editText1"
        android:layout_centerHorizontal="true"/>
    <Button
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        
        android:textSize="15dp"
        android:id="@+id/button1"
        android:background="@drawable/submitbbtn"/>
      <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Details"
        android:text="Admin Details"
        android:textColor="#fff"
        android:textSize="25sp"
        android:layout_below="@id/button1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Contact"
        android:text="Contact : "
        android:textColor="#fff"
        android:textSize="15sp"
        android:layout_below="@id/Details"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Email"
        android:text="Email : "
        android:textColor="#fff"
        android:textSize="15sp"
        android:layout_below="@+id/Contact"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"/>
       <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Contact_No"
        android:text="0311-2788504"
        android:textColor="#fff"
        android:textSize="15sp"
        android:layout_below="@id/Details"
        android:layout_toRightOf="@id/Contact"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Email_Address"
        android:text="problemupdatepu@gmail.com"
        android:textColor="#fff"
        android:textSize="15sp"
        android:layout_below="@+id/Contact_No"
        android:layout_toRightOf="@id/Contact"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"/>


</RelativeLayout>
